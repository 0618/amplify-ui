{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "rootDir": "./src",
    "outDir": "./dist",
    "skipLibCheck": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
