  "version": "1.0.10",
  "description": "Vue specific wrapper for @aws-amplify/ui-components",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
