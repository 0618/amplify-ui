{
  "private": true,
  "name": "@aws-amplify/ui-react",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/ui-react.js",
  "module": "./dist/ui-react.esm.js",
  "unpkg": "./dist/ui-react.umd.js",
  "exports": {
    ".": "./dist/ui-react.modern.js",
    "./styles.css": "./dist/ui-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle --jsx 'React.createElement' --jsxImportSource react --globals react/jsx-runtime=jsx",
    "predev": "rimraf dist",
    "dev": "yarn build watch --no-compress --raw",
    "primitives:new": "hygen primitives new",
    "test": "jest"
  },
  "dependencies": {
    "@aws-amplify/ui-core": "^0.0.1",
    "@aws-amplify/ui-theme-base": "^0.0.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@xstate/react": "^1.4.0",
    "classnames": "^2.3.1",
    "qrcode": "^1.4.4"
  },
  "peerDependencies": {
    "aws-amplify": "3.x.x || 4.x.x",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "https://github.com/sinankeskin/postcss-sass.git",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.2",
    "@types/testing-library__jest-dom": "^5.14.1",
    "autoprefixer": "^10.2.4",
    "hygen": "^6.1.0",
    "jest": "^27.0.4",
    "microbundle": "^0.13.3",
    "postcss-easy-import": "^3.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "workspaces": {
    "nohoist": [
      "@types/testing-library__jest-dom",
      "@types/testing-library__jest-dom/**"
    ]
  }
}
